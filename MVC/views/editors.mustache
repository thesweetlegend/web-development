{{>header}}

{{>navigation}}

<div class="row">
  <div class="leftcolumn">
    <div class="card">
      <h2>Editors</h2>
      <p>Only editors should be able to view this page!</p>
      <h3>Users</h3>
  <table class="table">
    <tr>
      <th>Username </th>
      <th>Password </th>
      <th>Level </th>
      <th>Delete</th>
    </tr>
    {{#user}}
    <tr>
      <td>{{username}}</td>
      <td>{{password}}</td>
      <td>{{level}}</td>
      <td><a href="/editors/delUser/{{username}}">D</a></td>
    </tr>
    {{/user}}
  </table>
  <h3>Articles</h3>
  <table class="table">
    <tr>
      <th>Title </th>
      <th>Author </th>
      <th>Delete</th>
    </tr>
    {{#article}}
    <tr>
      <td>{{title}}</td>
      <td>{{username}}</td>
      <td><a href="/editors/delArticle/{{rowid}}">D</a></td>
    </tr>
    {{/article}}
  </table>
    </div>
  </div>
  {{>sidebar}}

</div>

{{>footer}}
